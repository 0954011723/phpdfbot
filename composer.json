{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "ext-curl": "*",
        "ext-dom": "*",
        "ext-intl": "*",
        "ext-json": "*",
        "bensampo/laravel-enum": "^1.30",
        "cweagans/composer-patches": "^1.6",
        "dacastro4/laravel-gmail": "^3.2",
        "dingo/api": "^2.4",
        "doctrine/dbal": "^2.10",
        "fabpot/goutte": "^4.0",
        "fideloper/proxy": "^4.2",
        "graham-campbell/github": "^8.5",
        "graham-campbell/markdown": "^11.2",
        "guzzlehttp/guzzle": "^6.5",
        "irazasyed/telegram-bot-sdk": "^3.0",
        "jrm2k6/cloudder": "^0.6.0",
        "laravel/framework": "^6.2",
        "laravel/tinker": "^2.1",
        "laravelcollective/html": "^6.0",
        "league/html-to-markdown": "^4.9",
        "php-http/guzzle6-adapter": "^2.0",
        "prettus/l5-repository": "^2.6",
        "rap2hpoutre/laravel-log-viewer": "^1.3",
        "spatie/emoji": "^2.1",
        "spatie/laravel-fractal": "^5.6",
        "symfony/cache-contracts": "^2.0",
        "symfony/var-exporter": "^5.0"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^8.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        },
        "composer-exit-on-patch-failure": true,
        "patches": {
          "graham-campbell/github": {
            "Fix the compatibility with Laravel 6+": "patches/graham-campbell-github-102.patch"
          }
        }
    },
    "autoload": {
        "files": [
            "app/Helpers/Helper.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "App\\Models\\": "app/Models"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
